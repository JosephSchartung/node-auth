{
  "name": "node-auth",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/JosephSchartung/node-auth.git",
  "author": "Joseph Schartung <104148306+JosephSchartung@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "client-install": "cd client && yarn install",
    "server-install": "cd server && yarn install",
    "install": "concurrently \"yarn run client-install\" \"yarn run server-install\"",
    "server": "cd server && yarn run dev",
    "client": "cd client && yarn run start",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
  "dependencies": {
    "concurrently": "^7.2.1"
  }
}
